{
  "name": "@serp/root",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "eslint": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' turbo run eslint",
    "format": "prettier --write \"**/*.{ts,tsx,md,vue,json}\"",
    "lint": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' turbo run lint",
    "lint:windows": "turbo run lint",
    "prepare": "husky install",
    "prettier": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' turbo run prettier",
    "test": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' turbo run test",
    "test:update": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' turbo run test:update",
    "test:watch": "turbo run test:watch"
  },
  "resolutions": {
    "unimport": "4.1.1",
    "vue-tsc": "2.2.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "turbo": "^2.4.4"
  },
  "packageManager": "pnpm@8.15.6"
}
