{
  "name": "@serp/root",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "node scripts/dev.js",
    "eslint": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' turbo run eslint",
    "format": "prettier --write \"**/*.{ts,tsx,md,vue}\"",
