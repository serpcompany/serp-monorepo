{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".nuxt/**", ".output/**", "dist/**"]
    },
    "lint": {
      "outputLogs": "new-only"
    },
    "eslint": {},
    "prettier": {},
    "test": {
      "dependsOn": ["lint"],
       "outputLogs": "new-only"
    },
    "test:watch": {},
    "dev": {
      "cache": true,
      "persistent": true
    }
  }
}
